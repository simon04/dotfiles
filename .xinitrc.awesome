#!/bin/bash

setxkbmap -layout us -variant altgr-intl
xmodmap ~/.Xmodmap
xmodmap -e "pointer = 1 11 3 4 5 6 7 8 9 10 2 12"
urxvtd -f
clipit &
export _JAVA_AWT_WM_NONREPARENTING=1
(sleep 10 && dropboxd) &
pidof mpd || mpd &
pidof mpdscribble || mpdscribble &
exec dbus-launch --exit-with-session --sh-syntax awesome
